{
    "name": "slonline/dhlglobalmail",
    "description": "Provides a client to interact with the DHL Global Mail API in an object-oriented way.",
    "keywords": [
        "dhlglobalmail",
        "client",
        "api",
        "slonline",
        "parcel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lubos Odraska",
            "email": "odraska@slonline.sk"
        }
    ],
    "autoload": {
        "psr-4": {
            "SLONline\\DHLGlobalMail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\SLONline\\DHLGlobalMail\\": "test"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "php": ">=7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.7",
        "symfony/var-dumper": "^4.2"
    }
}
